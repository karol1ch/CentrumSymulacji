<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane stylesheets="/sample/css/appStyle.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <HBox prefHeight="50" prefWidth="510.0" spacing="100.0" styleClass="background" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="returnToMainMenu" mnemonicParsing="false" prefHeight="29.0" prefWidth="163.0" text="Main Menu" />
                <Button fx:id="setDesc" mnemonicParsing="false" prefHeight="29.0" prefWidth="163.0" text="Set Description"/>
                <Label fx:id="scenarioName" alignment="CENTER" contentDisplay="CENTER" prefHeight="31.0" prefWidth="748.0" styleClass="label-heading" textAlignment="CENTER">
                    <font>
                        <Font name="Cooper Black" size="27.0" />
                    </font>
                </Label>
            </children>
            <padding>
                <Insets bottom="15.0" left="12.0" right="12.0" top="15.0" />
            </padding>
            <BorderPane.margin>
                <Insets />
            </BorderPane.margin>
        </HBox>
    </top>

</BorderPane>
