<GridPane alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" halignment="CENTER" maxWidth="194.0" minWidth="10.0" prefWidth="97.0" />
    <ColumnConstraints hgrow="SOMETIMES" halignment="CENTER" maxWidth="334.0" minWidth="10.0" prefWidth="303.0" />
