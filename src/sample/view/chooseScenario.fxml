<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="borderPane" styleClass="background" stylesheets="/sample/css/appStyle.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">


    <center>
        <ListView fx:id="listView" BorderPane.alignment="CENTER" />
    </center>
   <bottom>
      <BorderPane maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
         <left>
                   <Button fx:id="runScenarioButton" disable="true" mnemonicParsing="false" prefHeight="26.0" prefWidth="196.0" text="Run scenario" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="20.0" left="10.0" top="20.0" />
               </BorderPane.margin></Button>
         </left>
         <center>
                   <Button fx:id="editScenarioButton" disable="true" mnemonicParsing="false" prefHeight="26.0" prefWidth="196.0" text="Edit scenario" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
               </BorderPane.margin></Button>
         </center>
         <right>
                   <Button fx:id="newScenarioButton" disable="true" mnemonicParsing="false" prefHeight="26.0" prefWidth="196.0" text="Add scenario" BorderPane.alignment="CENTER">
               <padding>
                  <Insets right="20.0" />
               </padding>
               <BorderPane.margin>
                  <Insets bottom="20.0" left="10.0" right="20.0" top="20.0" />
               </BorderPane.margin></Button>
         </right>
      </BorderPane>
   </bottom>
      </BorderPane>
